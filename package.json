      "npm run clean && cross-env NODE_ENV=\"production\" webpack-dev-server",
    "clean": "rm -rf ./build",
    "deploy": "git push heroku master",
    "postinstall": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
